cd /
cd /var/www/html/Immidart/docker-wordpress
docker build -t docker-wordpress /var/www/html/Immidart/docker-wordpress
docker run -v /var/lib/mysql --name=my_datastore -d busybox echo 'My Datastore'
docker run --name my_mysql -e MYSQL_ROOT_PASSWORD=test123 --volumes-from my_datastore -d mysql
docker run --link=my_mysql:mysql -p 7777:80 -d wordpress
